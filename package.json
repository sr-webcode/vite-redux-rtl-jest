    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "jest-watch-typeahead": "^1.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
